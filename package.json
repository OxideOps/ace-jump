{
    "name": "quick-jump",
    "displayName": "Quick Jump",
    "description": "Fast cursor navigation for VS Code - jump to any position in the visible editor area with a few keystrokes",
    "version": "0.0.1",
    "repository": {
        "type": "git",
        "url": "https://github.com/OxideOps/quick-jump.git"
    },
    "license": "MIT",
    "publisher": "OxideOps",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Other",
        "Keymaps"
    ],
    "keywords": [
        "navigation",
        "cursor",
        "jump",
        "productivity",
        "motion",
        "easymotion"
    ],
    "activationEvents": [],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "quick-jump.startJump",
                "title": "Quick Jump: Start Jump"
            },
            {
                "command": "quick-jump.escapeJumpMode",
                "title": "Quick Jump: Exit Jump Mode"
            }
        ],
        "keybindings": [
            {
                "command": "quick-jump.startJump",
                "key": "ctrl+;",
                "mac": "cmd+;",
                "when": "editorTextFocus"
            },
            {
                "command": "quick-jump.escapeJumpMode",
                "key": "escape",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "lint": "eslint src --ext ts"
    },
    "devDependencies": {
        "@types/vscode": "^1.60.0",
        "@types/node": "^22.15.2",
        "@typescript-eslint/eslint-plugin": "^8.31.0",
        "@typescript-eslint/parser": "^8.31.0",
        "eslint": "^9.25.1",
        "typescript": "^5.8.3",
        "ts-loader": "^9.3.1",
        "webpack": "^5.76.0",
        "webpack-cli": "^6.0.1"
    }
}